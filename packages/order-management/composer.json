{
    "name": "internal/order-management",
    "description": "Order Management System Package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Development Team"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "OrderManagement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OrderManagement\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OrderManagement\\Providers\\OrderManagementServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
